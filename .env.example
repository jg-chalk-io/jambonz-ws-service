# Server Configuration
PORT=3000
NODE_ENV=development

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# Ultravox
ULTRAVOX_API_KEY=your-ultravox-api-key

# Jambonz
JAMBONZ_ACCOUNT_SID=your-jambonz-account-sid
JAMBONZ_API_KEY=your-jambonz-api-key

# SIP Trunk for outbound calls (optional - will use default if not specified)
SIP_TRUNK_NAME=Twilio-PetOne

# Outbound Caller ID - MUST be a Twilio number you own or verified caller ID
# This is used as the "From" number when transferring calls
TWILIO_CALLER_ID=+16479526096

# WebSocket endpoint path
WS_PATH=/ws
